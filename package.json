{
  "name": "supertrucker",
  "version": "1.9.4",
  "browserslist": [
    "defaults"
  ],
  "main": "server/server.js",
  "scripts": {
    "ng": "ng",
    "start": "node server/server.js",
    "start-backend": "nodemon server/server.js",
    "start-frontend": "npm run dev --prefix client",
    "dev": "concurrently \"npm run start-frontend\" \"npm run start-backend\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Andrew Bartson",
  "license": "ISC",
  "description": "Interactive map for trip planning - Enter origin and destination, and get back a route with schedule and expected weather",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "gravatar": "^1.8.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mongoose": "^5.10.13",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "polyline": "^0.2.0",
    "rxjs": "~6.4.0",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "eslint": "^8.11.0",
    "nodemon": "^2.0.6"
  },
  "keywords": [
    "trip",
    "planning",
    "planner",
    "schedule",
    "road",
    "interstate",
    "travel",
    "weather",
    "transportation",
    "dispatch",
    "trucker"
  ]
}